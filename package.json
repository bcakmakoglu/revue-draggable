{
  "name": "@braks/revue-draggable",
  "version": "0.1.13",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/bcakmakoglu/revue-draggable"
  },
  "keywords": [
    "vue",
    "vue3",
    "draggable",
    "component",
    "directive",
    "composition api"
  ],
  "license": "MIT",
  "author": "Burak Cakmakoglu",
  "main": "dist/revue-draggable.es.js",
  "browser": "dist/revue-draggable.esm.js",
  "unpkg": "dist/revue-draggable.global.js",
  "jsdelivr": "dist/revue-draggable.global.js",
  "module": "dist/revue-draggable.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dist": "rollup -c --environment NODE_ENV:production",
    "serve": "vite preview",
    "prepublishOnly": "yarn build:dist",
    "test": "cypress run",
    "lint:js": "eslint --ext \".js,.jsx,.ts,.tsx\" --fix --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "ci": "start-server-and-test dev http-get://localhost:3000 test"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.4",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-eslint": "latest",
    "cypress": "^7.7.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^7.0.1",
    "np": "^7.5.0",
    "pascalcase": "^1.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.52.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "start-server-and-test": "^1.12.6",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "vite": "^2.3.8",
    "vue": "^3.0.5",
    "vue-router": "4",
    "vue-tsc": "^0.0.24"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.6.0 || >=3.0.5"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org/"
  },
  "np": {
    "branch": "master",
    "yarn": true,
    "message": "v%s",
    "testScript": "ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
